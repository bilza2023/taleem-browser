{
  "name": "taleem-browser",
  "version": "0.2.0",
  "description": "Pure JSON slide browser for taleem-slides (index-based, no timing).",
  "type": "module",
  "main": "./dist/taleem-browser.umd.js",
  "module": "./dist/taleem-browser.esm.js",
  "exports": {
    ".": {
      "import": "./dist/taleem-browser.esm.js",
      "require": "./dist/taleem-browser.umd.js"
    },
    "./style.css": "./dist/taleem.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ./scripts/build.js",
    "test": "vitest"
  },
  "dependencies": {
    "taleem-slides": "^0.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "vitest": "^1.6.0",
    "jsdom": "^24.0.0"
  },
  "license": "MIT"
}
